Static Function xAtivar(oView)
	Local oModel := FwModelActive()  as Object 
	local oGridRotaG := oModel:GetModel("MdGridRotaG") as Object
	local oGridRotaD := oModel:GetModel("MdGridRotaD") as Object
	local oGridZ06G := oModel:GetModel("MdGridZ06G") as Object
	local oGridZ06D := oModel:GetModel("MdGridZ06D") as Object
	local oGridZ05G := oModel:GetModel("MdGridZ05G") as Object
	local cAliasRotaG as Character
	local cAliasRotaD as Character
	local cAliasZ06G  as Character
	local cAliasZ06D  as Character
	local cAliasZ05G  as Character
	local nI as numeric
	local nLen := GetMV("VA_REGHIST",,5) as numeric

	oView:AVIEWS[3][3]:oBrowse:oBrowseUi:oGrid:SetSelectionMode(0) //VwGridZ06G
	oView:AVIEWS[4][3]:oBrowse:oBrowseUi:oGrid:SetSelectionMode(0) //VwGridZ06D
	oView:AVIEWS[5][3]:oBrowse:oBrowseUi:oGrid:SetSelectionMode(0) //VwGridROTG
	
	oGridRotaG:SetNoInsertLine(.F.)
	oGridRotaD:SetNoInsertLine(.F.)
	oGridZ06G:SetNoInsertLine(.F.)
	oGridZ06D:SetNoInsertLine(.F.)
	oGridZ05G:SetNoInsertLine(.F.)

	cAliasRotaG := oExecRotaG:OpenAlias()
	While !(cAliasRotaG)->(EOF())
		oGridRotaG:LoadValue("Z0S_EQUIP",(cAliasRotaG)->Z0S_EQUIP)
		oGridRotaG:LoadValue("ZV0_DESC" ,Alltrim((cAliasRotaG)->ZV0_DESC))
		oGridRotaG:LoadValue("Z0T_ROTA" ,(cAliasRotaG)->Z0T_ROTA)
		
		oExecRotaD:SetString(1,(cAliasRotaG)->Z0T_ROTA)

		cAliasRotaD:=oExecRotaD:OpenAlias()
		While !(cAliasRotaD)->(EOF())
			oGridRotaD:AddLine()
			oGridRotaD:LoadValue("Z0T_ROTA" ,(cAliasRotaD)->Z0T_ROTA)
			oGridRotaD:LoadValue("Z06_TRATO",(cAliasRotaD)->Z06_TRATO)
			oGridRotaD:LoadValue("Z06_KGMNT",(cAliasRotaD)->Z06_KGMNT)
			(cAliasRotaD)->(DbSkip())

			IF !(cAliasRotaD)->(EOF())
				oGridRotaD:AddLine()
			endif
		End
		(cAliasRotaD)->(DbCloseArea())
		
		oExecZ06G:SetString(1,(cAliasRotaG)->Z0T_ROTA)
		cAliasZ06G := oExecZ06G:OpenAlias()
		While !(cAliasZ06G)->(EOF())
			if (cAliasZ06G)->Z05_KGMSDI < (cAliasZ06G)->Z05_PROGANT
				cLegenda := "BR_VERMELHO"
			elseif (cAliasZ06G)->Z05_KGMSDI = (cAliasZ06G)->Z05_PROGANT
				cLegenda := "BR_AMARELO"
			elseif (cAliasZ06G)->Z05_KGMSDI > (cAliasZ06G)->Z05_PROGANT
				cLegenda := "BR_VERDE"
			else 
				cLegenda := "BR_BRANCO"
			endif

			oGridZ06G:LoadValue("XX5_LEGEND" 	,cLegenda)
			oGridZ06G:LoadValue("Z05_CURRAL" 	,Alltrim((cAliasZ06G)->Z05_CURRAL))
			oGridZ06G:LoadValue("Z06_LOTE"	  	,Alltrim((cAliasZ06G)->Z05_LOTE))
			oGridZ06G:LoadValue("Z05_CABECA" 	,(cAliasZ06G)->Z05_CABECA)
			oGridZ06G:LoadValue("Z05_DIASDI" 	,(cAliasZ06G)->Z05_DIASDI)
			oGridZ06G:LoadValue("Z05_PESMAT" 	,(cAliasZ06G)->Z05_PESMAT)
			oGridZ06G:LoadValue("Z0I_NOTTAR" 	,(cAliasZ06G)->Z0I_NOTTAR)
			oGridZ06G:LoadValue("Z0I_NOTMAN" 	,(cAliasZ06G)->Z0I_NOTMAN)
			oGridZ06G:LoadValue("Z05_PROGANT" 	,(cAliasZ06G)->Z05_PROGANT)
			oGridZ06G:LoadValue("XX5_KGMSDI" 	,(cAliasZ06G)->Z05_KGMSDI)
			oGridZ06G:LoadValue("Z06_DIETA" 	,Alltrim((cAliasZ06G)->Z05_DIETA))
			oGridZ06G:LoadValue("Z05_NROTRA" 	,(cAliasZ06G)->Z05_NROTRA)
			oGridZ06G:LoadValue("Z05_TOTMNC" 	,(cAliasZ06G)->Z05_TOTMNC)
			oGridZ06G:LoadValue("Z05_RECNO" 	,(cAliasZ06G)->Z05_RECNO)
			oGridZ06G:LoadValue("Z05_TOTMSC" 	,(cAliasZ06G)->Z05_TOTMSC)
			oGridZ06G:LoadValue("Z0T_ROTA" 		,Alltrim((cAliasZ06G)->Z0T_ROTA))
			oGridZ06G:LoadValue("XX5_PVATU" 	,Round(((cAliasZ06G)->Z05_TOTMSC / (cAliasZ06G)->Z05_PESMAT) * 100,2))
			oGridZ06G:LoadValue("Z05_CMSPN" 	,(cAliasZ06G)->Z05_CMSPN)

			oExecZ06D:SetString(1,(cAliasRotaG)->Z0T_ROTA)
			oExecZ06D:SetString(2,Alltrim((cAliasZ06G)->Z05_LOTE))
			
			cAliasZ06D := oExecZ06D:OpenAlias()
			While !(cAliasZ06D)->(EOF())
				oGridZ06D:LoadValue("Z06_TRATO"	,(cAliasZ06D)->Z06_TRATO)
				oGridZ06D:LoadValue("Z06_CURRAL"	,AllTrim((cAliasZ06D)->Z06_CURRAL))
				oGridZ06D:LoadValue("Z06_LOTE"		,(cAliasZ06D)->Z06_LOTE)
				oGridZ06D:LoadValue("Z06_DIETA"	,AllTrim((cAliasZ06D)->Z06_DIETA))
				oGridZ06D:LoadValue("Z06_KGMSTR"	,(cAliasZ06D)->Z06_KGMSTR)
				oGridZ06D:LoadValue("Z06_KGMNTR"	,(cAliasZ06D)->Z06_KGMNTR)
				oGridZ06D:LoadValue("Z06_MEGCAL"	,(cAliasZ06D)->Z06_MEGCAL)
				oGridZ06D:LoadValue("Z06_KGMNT"	,(cAliasZ06D)->Z06_KGMNT)
				oGridZ06D:LoadValue("Z06_RECNO"	,(cAliasZ06D)->Z06_RECNO)
				
				(cAliasZ06D)->(DbSkip())
				IF !(cAliasZ06D)->(EOF())
					oGridZ06D:AddLine()
				endif
			End
	
			for nI := 1 to nLen
				oExecZ05G:SetDate(nI, Z0R->Z0R_DATA-nI)
			next
			oExecZ05G:SetString(nI, (cAliasZ06D)->Z06_LOTE)
			
			(cAliasZ06D)->(DbCloseArea())

			cAliasZ05G := oExecZ05G:OpenAlias()
			While !(cAliasZ05G)->(EOF())
				oGridZ05G:LoadValue("Z05_DATA"		,sToD((cAliasZ05G)->Z05_DATA))
				oGridZ05G:LoadValue("Z05_DIETA"		,AllTrim((cAliasZ05G)->Z05_DIETA))
				oGridZ05G:LoadValue("Z05_KGMSDI"	,(cAliasZ05G)->Z05_KGMSDI )
				oGridZ05G:LoadValue("Z0I_NOTMAN"	,(cAliasZ05G)->Z0I_NOTMAN )
				oGridZ05G:LoadValue("Z0I_NOTTAR"	,(cAliasZ05G)->Z0I_NOTTAR )
				oGridZ05G:LoadValue("Z05_CABECA"	,(cAliasZ05G)->Z05_CABECA )
				oGridZ05G:LoadValue("Z05_MEGCAL"	,(cAliasZ05G)->Z05_MEGCAL )
				oGridZ05G:LoadValue("Z05_KGMNDI"	,(cAliasZ05G)->Z05_KGMNDI )
				oGridZ05G:LoadValue("Z0W_QTDPRE"	,(cAliasZ05G)->Z0W_QTDPRE )
				oGridZ05G:LoadValue("Z0W_QTDREA"	,(cAliasZ05G)->Z0W_QTDREA )
				oGridZ05G:LoadValue("XX1_DIF"		,(cAliasZ05G)->DIF )
				
				(cAliasZ05G)->(DbSkip())

				IF !(cAliasZ05G)->(EOF())
					oGridZ05G:AddLine()
				endif
			End
			(cAliasZ05G)->(DbCloseArea())

			(cAliasZ06G)->(DbSkip())
			IF !(cAliasZ06G)->(EOF())
				oGridZ06G:AddLine()
			endif
		End
		(cAliasZ06G)->(DbCloseArea())

		(cAliasRotaG)->(DbSkip())

		IF !(cAliasRotaG)->(EOF())
			oGridRotaG:AddLine()
		endif
	end
	(cAliasRotaG)->(DbCloseArea())

	oView:Refresh()

	oGridRotaG:GoLine(1)
	oGridRotaD:GoLine(1)
	oGridZ06G:GoLine(1)
	oGridZ06D:GoLine(1)
	oGridZ05G:GoLine(1)

	oGridRotaG:SetNoInsertLine(.T.)
	oGridRotaD:SetNoInsertLine(.T.)
	oGridZ06G:SetNoInsertLine(.T.)
	oGridZ06D:SetNoInsertLine(.T.)
	oGridZ05G:SetNoInsertLine(.T.)
Return nil


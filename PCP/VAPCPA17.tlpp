Return .T.

static function HidGrdMStr() AS Object
    local aArea   		:= GetArea() 
    local oStruct 		:= FWFormModelStruct():New() AS Object
	local cQry 	  		:= "" as Character
	Local cAlias  		:= ""	as Character
	local aCombo  		:= {} as Array
	local aComboEquip	:= {} as Array
	local aComboRota	:= {} as Array
	local cZ0TROTA  	:= IIF(Type("cFiltroRota")!="U" .and. !Empty(cFiltroRota),cFiltroRota,(cTrbBrowse)->Z0T_ROTA)
	local cZ0SEQUIP  	:= IIF(Type("cFiltroEquip")!="U" .and. !Empty(cFiltroEquip),cFiltroEquip,nil)
	local cEquipamento  := "" as Character
	Local nLen,ni as Numeric
	LOCAL cDesc  		:= "" as Character

	IF Type("cFiltroRota") == "U"
		Private cFiltroRota	as Character
	endif
	
	IF Type("cFiltroEquip") == "U"
		Private cFiltroEquip	as Character
	endif

	cQry := " SELECT Z0T_ROTA, Z0S_EQUIP" + CRLF
	cQry += " FROM "+RetSqlName("Z0T")+" Z0T" + CRLF
	cQry += " JOIN "+RetSqlName("Z0S")+" Z0S ON Z0S_FILIAL = Z0T_FILIAL" + CRLF
	cQry += " AND Z0S_DATA = Z0T_DATA" + CRLF
	cQry += " AND Z0S_ROTA = Z0T_ROTA" + CRLF
	cQry += " AND Z0S.D_E_L_E_T_ = ''" + CRLF
	cQry += " WHERE Z0T_DATA = '"+dToS(Z0R->Z0R_DATA)+"'" + CRLF
	cQry += " AND Z0T.D_E_L_E_T_ = ''" + CRLF
	cQry += " AND Z0T_ROTA != ''" + CRLF
	cQry += " GROUP BY Z0T_ROTA,Z0S_EQUIP" + CRLF
	cQry += " ORDER BY Z0S_EQUIP,Z0T_ROTA" + CRLF

	cAlias := MpSysOpenQuery(cQry)

	While !(cALias)->(EOF())
		cEquipamento := AllTrim((cALias)->Z0S_EQUIP)
		aAdd(aCombo, {AllTrim((cALias)->Z0S_EQUIP),{}} )
		
		aAdd(aComboEquip, AllTrim((cALias)->Z0S_EQUIP) )
		
		nLen := Len(aCombo)
		While !(cALias)->(EOF()) .AND. cEquipamento == AllTrim((cALias)->Z0S_EQUIP)
			aAdd(aCombo[nLen][2], AllTrim((cALias)->Z0T_ROTA) )
			(cALias)->(DbSkip())
		enddo
	enddo

	(cALias)->(DbCloseArea())

	if cZ0SEQUIP == nil
		For nI := 1 to Len(aCombo)
			if aScan(aCombo[nI][2], cZ0TROTA) > 0
				cZ0SEQUIP := aCombo[nI][1]
				aComboRota := aCombo[nI][2]
				cFiltroEquip := cZ0SEQUIP
				Exit
			endif
		Next nI
	endif

    oStruct:AddField(;
                     "Caminhão",;              		// [01]  C   Titulo do campo
                     "",;              				// [02]  C   ToolTip do campo
                     "XX1_EQUIP",;   				// [03]  C   Id do Field
                     "C",; 							// [04]  C   Tipo do campo
                     6 ,; 							// [05]  N   Tamanho do campo
                     0,; 							// [06]  N   Decimal do campo
                     /* {|| U_RefreshRotaVAPCPA17()} */ ,;// [07]  B   Code-block de validação do campo
                     nil,;                      	// [08]  B   Code-block de validação When do campo
                     aComboEquip,;      					// [09]  A   Lista de valores permitido do campo
                     .F.,;                      	// [10]  L   Indica se o campo tem preenchimento obrigatório
                     FwBuildFeature(STRUCT_FEATURE_INIPAD,   "'"+cZ0SEQUIP+"'"  ),;// [11]  B   Code-block de inicializacao do campo
                     .F.,;                      	// [12]  L   Indica se trata-se de um campo chave
                     .F.,;                      	// [13]  L   Indica se o campo NÃO pode receber valor em uma operação de update.
                     .F.)                       	// [14]  L   Indica se o campo é virtual
	
	cDesc := Posicione("ZV0",1,FwxFilial("ZV0")+cZ0SEQUIP,"ZV0_DESC")
	oStruct:AddField(;
                     "Descrição",;              		// [01]  C   Titulo do campo
                     "",;              				// [02]  C   ToolTip do campo
                     "XX1_DESC",;   				// [03]  C   Id do Field
                     "C",; 							// [04]  C   Tipo do campo
                     60 ,; 							// [05]  N   Tamanho do campo
                     0,; 							// [06]  N   Decimal do campo
                     /* {|| U_RefreshRotaVAPCPA17()} */ ,;// [07]  B   Code-block de validação do campo
                     nil,;                      	// [08]  B   Code-block de validação When do campo
                     nil,;      					// [09]  A   Lista de valores permitido do campo
                     .F.,;                      	// [10]  L   Indica se o campo tem preenchimento obrigatório
                     FwBuildFeature(STRUCT_FEATURE_INIPAD,   "'"+cDesc+"'" ),;// [11]  B   Code-block de inicializacao do campo
                     .F.,;                      	// [12]  L   Indica se trata-se de um campo chave
                     .F.,;                      	// [13]  L   Indica se o campo NÃO pode receber valor em uma operação de update.
                     .F.)                       	// [14]  L   Indica se o campo é virtual

	//oStruct:AddField(;
    //                 "Rotas",;              		// [01]  C   Titulo do campo
    //                 "",;              				// [02]  C   ToolTip do campo
    //                 "XX1_ROTAS",;   				// [03]  C   Id do Field

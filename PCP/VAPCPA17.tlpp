static function AtuMatSec()
	local aArea     := GetArea() as Array
	local nQtdTrato := 0 as numeric
	local aKgMS     := {} as Array
	local nQtdeMS   := 0 as numeric
	local nQtdeMN   := 0 as numeric
	local nQtdMCal  := 0 as numeric
	local oModel 	:= FWModelActive() as object
	local oGridZ06G	:= oModel:GetModel("MdGridZ06G") as object
	local oGridZ06D	:= oModel:GetModel("MdGridZ06D") as object
	local nI

	For nI := 1 to oGridZ06D:Length()
		oGridZ06D:GoLine(nI)
		if !oGridZ06D:IsDeleted() 
			nQtdTrato += 1
		endif
	Next

	aKgMS := U_DivTrato(oGridZ06G:GetValue("XX5_KGMSDI"), nQtdTrato)

	if nQtdTrato > 0
		Z06->(DbSetOrder(1)) // Z06_FILIAL+DToS(Z06_DATA)+Z06_VERSAO+Z06_CURRAL+Z06_LOTE+Z06_TRATO
		For nI:= 1 to  oGridZ06D:Length()
			oGridZ06D:GoLine(nI)
			if !oGridZ06D:IsDeleted() 
				cDieta := oGridZ06D:GetValue("Z06_DIETA")+SPACE(TamSX3("Z06_DIETA")[1]-Len(oGridZ06D:GetValue("Z06_DIETA")))
				oGridZ06D:SetValue("Z06_KGMSTR", aKgMS[nI])
				oGridZ06D:SetValue("Z06_KGMNTR", U_CalcQtMN(cDieta, aKgMS[nI]))
				oGridZ06D:SetValue("Z06_MEGCAL", U_GetMegaCal(cDieta) * aKgMS[nI])
				oGridZ06D:SetValue("Z06_KGMNT" , u_CalcQtMN(cDieta, aKgMS[nI]) * oGridZ06G:GetValue("Z05_CABECA"))

				Z06->(DbGoTo(oGridZ06D:GetValue("Z06_RECNO")))
				RecLock("Z06", .F.)
					Z06->Z06_KGMSTR := oGridZ06D:GetValue("Z06_KGMSTR") 
					Z06->Z06_KGMNTR := oGridZ06D:GetValue("Z06_KGMNTR")
					Z06->Z06_KGMNT  := oGridZ06D:GetValue("Z06_KGMNTR") * Z05->Z05_CABECA
					Z06->Z06_MEGCAL := oGridZ06D:GetValue("Z06_MEGCAL")
				Z06->(MsUnlock())
				nQtdeMS += Z06->Z06_KGMSTR
				nQtdeMN += Z06->Z06_KGMNTR
				nQtdMCal += Z06->Z06_MEGCAL
			endif
		Next
		
		RecLock("Z05", .F.)
			Z05->Z05_KGMSDI := Z05->Z05_TOTMSI := oGridZ06G:GetValue("XX5_KGMSDI") 
			Z05->Z05_KGMNDI := Z05->Z05_TOTMNI := nQtdeMN
			Z05->Z05_CMSPN  := (oGridZ06G:GetValue("XX5_KGMSDI") / Z05->Z05_PESMAT) *100
			Z05->Z05_MEGCAL := nQtdMCal
			Z05->Z05_MANUAL := '1'
			Z05->Z05_ORIGEM := '3'
		MsUnlock()

		AjuMateria(oModel)
	endif
	
	oGridZ06D:GoLine(1)
	
	RestArea(aArea)
return nil

Static Function AtualizaTela()
	Local oModel	  	:= FWModelActive() 	as Object
	Local oView 	 	:= FWViewActive() 	as Object

	oModel:DeActivate()

	oModel:Activate()
	
	oView:Refresh()
Return

Static Function AltQtdCab()
	local aAreaZ05  := Z05->(GetArea()) as Array
	Local oModel	:= FWModelActive() 	as Object
	LOcal oGridZ06G	:= oModel:GetModel("MdGridZ06G") as Object
	Local cFillPerg := "VAPCPA05B " as character
	Local _cMsg     := "" as character

    AtuSX1(@cFillPerg)
    if Pergunte(cFillPerg)
        if Z05->(DbSeek(FWxFilial("Z05")+DToS(Z0R->Z0R_DATA)+Z0R->Z0R_VERSAO+;
						PADR(oGridZ06G:GetValue("Z05_CURRAL"), TamSx3("Z05_CURRAL")[1])+;
						PADR(oGridZ06G:GetValue("Z06_LOTE"), TamSx3("Z06_LOTE")[1]))) 
                // ConOut("Z05_CABECA")
            _cMsg := cValToChar(Z05->Z05_CABECA) + " / " + cValToChar(MV_PAR01)
            RecLock("Z05")
                Z05->Z05_CABECA := MV_PAR01
            Z05->(MsUnLock())
			
			oGridZ06G:LoadValue("Z05_CABECA",Z05->Z05_CABECA)

            MsgInfo("Atualização de cabeças realizada com sucesso: " + _cMsg , "Atenção")
        Endif
    EndIf

   Z05->(RestArea(aAreaZ05))

Return

Static Function GeraX1(cPerg)

Local _aArea	:= GetArea()
Local aRegs     := {}
Local nX		:= 0
Local nPergs	:= 0
Local i := 0, j := 0

//Conta quantas perguntas existem ualmente.
DbSelectArea('SX1')
DbSetOrder(1)
SX1->(DbGoTop())
If SX1->(DbSeek(cPerg))
	While !SX1->(Eof()) .And. X1_GRUPO = cPerg
		nPergs++
		SX1->(DbSkip())
	EndDo
EndIf

aAdd(aRegs,{cPerg, "01", "Número de tratos?", "", "", "MV_CH1", "N", 1   					   , 0					        , 0, "G", "", "MV_PAR01", "", "","",""      ,"",""   ,"","","","","","","","","","","","","","","","","","","   ","","","","",""})
aAdd(aRegs,{cPerg, "02", "Curral de?"		, "", "", "MV_CH2", "C", TamSx3("Z08_CODIGO")[1]   , TamSx3("Z08_CODIGO")[2]    , 0, "G", "", "MV_PAR02", "", "","",""      ,"",""   ,"","","","","","","","","","","","","","","","","","","   ","","","","",""})
aAdd(aRegs,{cPerg, "03", "Curral ate?"		, "", "", "MV_CH3", "C", TamSx3("Z08_CODIGO")[1]   , TamSx3("Z08_CODIGO")[2]    , 0, "G", "", "MV_PAR03", "", "","",""      ,"",""   ,"","","","","","","","","","","","","","","","","","","   ","","","","",""})

//Se quantidade de perguntas for diferente, apago todas
SX1->(DbGoTop())
If nPergs <> Len(aRegs)
	For nX:=1 To nPergs
		If SX1->(DbSeek(cPerg))		
			If RecLock('SX1',.F.)
				SX1->(DbDelete())
				SX1->(MsUnlock())
			EndIf
		EndIf
	Next nX
EndIf

// gravaÃ§Ã£o das perguntas na tabela SX1
If nPergs <> Len(aRegs)
	dbSelectArea("SX1")
	dbSetOrder(1)
	For i := 1 to Len(aRegs)
		If !dbSeek(cPerg+aRegs[i,2])
			RecLock("SX1",.T.)
				For j := 1 to FCount()
					If j <= Len(aRegs[i])
						FieldPut(j,aRegs[i,j])
					Endif
				Next j
			MsUnlock()
		EndIf
	Next i
EndIf

RestArea(_aArea)

Return nil



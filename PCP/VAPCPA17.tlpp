	oModel:SetOptional("MdGridZ05G"	, .T.)
	oModel:SetOptional("MdGridZ06G"	, .T.)
	oModel:SetOptional("MdGridZ06D"	, .T.)
	oModel:SetOptional("MdGridRotaD", .T.)

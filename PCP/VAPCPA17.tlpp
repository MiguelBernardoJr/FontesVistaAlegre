	oView:EnableTitleView('VwGridZ06G'	, "Lista de Currais"		)
	oView:EnableTitleView('VwGridZ06D'	, "Relação de Tratos"		)
	oView:EnableTitleView('VwFieldRotas', "Seleção de Rotas"		)
	oView:EnableTitleView('VwKgTot3'	, "Total da rota"			)
	oView:EnableTitleView('VwKgTot2'	, "Totais por Lote"			)
	oView:EnableTitleView('VwGridZ05G'	, "Programação anterior"	)
	oView:EnableTitleView('VwKgTot1'   	, "Totais por Rota"			)

	if Inclui .or. Altera
		oView:AddUserButton( "Gerar Arquivos (F7)" 	, 'OK', {|| u_vapcpa08(Z0R->Z0R_DATA, Z0R->Z0R_VERSAO, "")} )
		oView:AddUserButton( "Nro Tratos (F8)"		, 'OK', {|| vap17tra() } )
		oView:AddUserButton( "Matéria Seca (F9)"	, 'OK', {|| vap17msc() } )
		oView:AddUserButton( "Dietas (F10)"			, 'OK', {|| vap17trt() } )
		oView:AddUserButton( "Atualizar (F11)" 		, 'OK', {|| AtualizaTela() } )
		oView:AddUserButton( "Alt Cabeca Lote"		, 'OK', {|| AltQtdCab() } )
		oView:AddUserButton( "Transf. Curral"		, 'OK', {|| vap17tcu() } )

		SetKey(VK_F7 , {|| u_vapcpa08(Z0R->Z0R_DATA, Z0R->Z0R_VERSAO, "")})
		SetKey(VK_F8 , {|| vap17tra()})
		SetKey(VK_F9 , {|| vap17msc()})
		SetKey(VK_F10, {|| vap17trt()})
		SetKey(VK_F11, {|| AtualizaTela()})
	endif
	
	oView:SetAfterViewActivate({|oView| xAtivar(oView)})

return oView

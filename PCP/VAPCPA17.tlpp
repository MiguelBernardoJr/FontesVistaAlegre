static function HidGrdVStr() as Object
	local aArea   := GetArea() as Array
	local oStruct := FWFormViewStruct():New() as Object
	local cQry 	  := "" as Character
	Local cAlias  := ""	as Character
	local aCombo  		:= {} as Array
	local aComboEquip	:= {} as Array
	local aComboRota	:= {} as Array
	local cEquipamento  := "" as Character
	Local nLen,ni as Numeric
	local cZ0TROTA  	:= IIF(Type("cFiltroRota")!="U" .and. !Empty(cFiltroRota),cFiltroRota,(cTrbBrowse)->Z0T_ROTA)
	local cZ0SEQUIP  	:= IIF(Type("cFiltroEquip")!="U" .and. !Empty(cFiltroEquip),cFiltroEquip,nil)

	cQry := " SELECT Z0T_ROTA, Z0S_EQUIP" + CRLF
	cQry += " FROM "+RetSqlName("Z0T")+" Z0T" + CRLF
	cQry += " JOIN "+RetSqlName("Z0S")+" Z0S ON Z0S_FILIAL = Z0T_FILIAL" + CRLF
	cQry += " AND Z0S_DATA = Z0T_DATA" + CRLF
	cQry += " AND Z0S_ROTA = Z0T_ROTA" + CRLF
	cQry += " AND Z0S.D_E_L_E_T_ = ''" + CRLF
	cQry += " WHERE Z0T_DATA = '"+dToS(Z0R->Z0R_DATA)+"'" + CRLF
	cQry += " AND Z0T.D_E_L_E_T_ = ''" + CRLF
	cQry += " AND Z0T_ROTA != ''" + CRLF
	cQry += " GROUP BY Z0T_ROTA,Z0S_EQUIP" + CRLF
	cQry += " ORDER BY Z0S_EQUIP,Z0T_ROTA" + CRLF

	cAlias := MpSysOpenQuery(cQry)

	While !(cALias)->(EOF())
		cEquipamento := AllTrim((cALias)->Z0S_EQUIP)
		aAdd(aCombo, {AllTrim((cALias)->Z0S_EQUIP),{}} )
		
		aAdd(aComboEquip, AllTrim((cALias)->Z0S_EQUIP) )
		
		nLen := Len(aCombo)
		While !(cALias)->(EOF()) .AND. cEquipamento == AllTrim((cALias)->Z0S_EQUIP)
			aAdd(aCombo[nLen][2], AllTrim((cALias)->Z0T_ROTA) )
			(cALias)->(DbSkip())
		enddo
	enddo

	if cZ0SEQUIP == nil
		For nI := 1 to Len(aCombo)
			if aScan(aCombo[nI][2], cZ0TROTA) > 0
				cZ0SEQUIP := aCombo[nI][1]
				aComboRota := aCombo[nI][2]
				cFiltroEquip := cZ0SEQUIP
				Exit
			endif
		Next nI
	endif

	(cALias)->(DbCloseArea())
		oStruct:AddField(;
			"XX1_EQUIP",;        // [01]  C   Nome do Campo
			"1",; 				// [02]  C   Ordem
			"Caminhão",;           // [03]  C   Titulo do campo
			"",;                // [04]  C   Descricao do campo
			{"Help"},;          // [05]  A   Array com Help
			"C",;      			// [06]  C   Tipo do campo
			"",;      			// [07]  C   Picture
			nil,;               // [08]  B   Bloco de PictTre Var
			,;                  // [09]  C   Consulta F3
			.T.,;  				// [10]  L   Indica se o campo é alteravel
			nil,;               // [11]  C   Pasta do campo
			nil,;               // [12]  C   Agrupamento do campo
			aComboEquip,;               // [13]  A   Lista de valores permitido do campo (Combo)
			nil,;               // [14]  N   Tamanho máximo da maior opção do combo
			nil,;               // [15]  C   Inicializador de Browse
			.t.,;               // [16]  L   Indica se o campo é virtual
			nil,;               // [17]  C   Picture Variável
			nil;                // [18]  L   Indica pulo de linha após o campo
		)
		oStruct:AddField(;
			"XX1_DESC",;        // [01]  C   Nome do Campo
			"2",; 				// [02]  C   Ordem
			"Descrição",;           // [03]  C   Titulo do campo
			"",;                // [04]  C   Descricao do campo
			{"Help"},;          // [05]  A   Array com Help
			"C",;      			// [06]  C   Tipo do campo
			"",;      			// [07]  C   Picture
			nil,;               // [08]  B   Bloco de PictTre Var
			,;                  // [09]  C   Consulta F3
			.T.,;  				// [10]  L   Indica se o campo é alteravel
			nil,;               // [11]  C   Pasta do campo
			nil,;               // [12]  C   Agrupamento do campo
			,;               	// [13]  A   Lista de valores permitido do campo (Combo)
			nil,;               // [14]  N   Tamanho máximo da maior opção do combo
			nil,;               // [15]  C   Inicializador de Browse
			.t.,;               // [16]  L   Indica se o campo é virtual
			nil,;               // [17]  C   Picture Variável
			nil;                // [18]  L   Indica pulo de linha após o campo
		)
		//oStruct:AddField(;
		//	"XX1_ROTAS",;        // [01]  C   Nome do Campo
		//	"3",; 				// [02]  C   Ordem
		//	"Rotas",;           // [03]  C   Titulo do campo
		//	"",;                // [04]  C   Descricao do campo
		//	{"Help"},;          // [05]  A   Array com Help

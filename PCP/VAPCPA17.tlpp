	cQry += 		", Z06.Z06_KGMNTR	" + CRLF
	cQry += 		", Z06.Z06_MEGCAL	" + CRLF
	cQry += 		", Z06.Z06_KGMNT	" + CRLF
	cQry += 		", Z06.R_E_C_N_O_ Z06_RECNO" + CRLF
	cQry += 	" from " + RetSqlName("Z06") + " Z06" + CRLF

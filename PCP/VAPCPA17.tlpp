    cQry += "               and Z0T.Z0T_ROTA   <> '      ' " + CRLF
    cQry += "               and Z0T.D_E_L_E_T_ = ' ' " + CRLF
    cQry += "          group by Z0S.Z0S_EQUIP " + CRLF
    cQry += "                 , ZV0.ZV0_IDENT " + CRLF
    cQry += "                 , Z0S.Z0S_OPERAD " + CRLF

	oView:AddGrid("VwGridZ06D" 		, oStrZ06D 	, "MdGridZ06D"  )
	oView:AddGrid("VwGridROTG" 		, oStrRotG  , "MdGridRotaG"	)
	oView:AddGrid("VwGridROTD" 		, oStrRotD  , "MdGridRotaD"	)
	oView:AddField("VwKgTot1" 		, oStrTot1  , "TOTAIS1" 	)
	oView:AddField("VwKgTot2" 		, oStrTot2  , "TOTAIS2" 	)
	oView:AddField("VwKgTot3" 		, oStrTot3  , "TOTAIS3" 	)

	oView:CreateHorizontalBox("CAB_ROTAS"   , 0)
	oView:CreateHorizontalBox("ITENS"       , 100)

	oView:CreateVerticalBox("ROTAS",  10, "ITENS")
	oView:CreateVerticalBox("PROGR",  51, "ITENS")
	oView:CreateVerticalBox("LOTES",  39, "ITENS")

	oView:CreateHorizontalBox("PROGR_CIMA"  , 88, "PROGR")
	oView:CreateHorizontalBox("PROGR_BAIXO" , 12, "PROGR")

	oView:CreateHorizontalBox("ROTAS_CIMA" , 100, "ROTAS")

	oView:CreateHorizontalBox("LOTES_CIMA"  , 20, "LOTES")
	oView:CreateHorizontalBox("LOTES_CENTER", 30, "LOTES")
	oView:CreateHorizontalBox("LOTES_BAIXO" , 20, "LOTES")
	oView:CreateHorizontalBox("ROTAS_MEIO"  , 30, "LOTES")
	//oView:CreateHorizontalBox("ROTAS_BAIXO", 10, "LOTES")
	
	oView:CreateVerticalBox("PROGR_BAIXO_DIR",  60, "PROGR_BAIXO")
	oView:CreateVerticalBox("PROGR_BAIXO_ESQ",  40, "PROGR_BAIXO")

	oView:SetOwnerView("VwFieldRotas"  	, "CAB_ROTAS"		)
	oView:SetOwnerView("VwGridROTG"  	, "ROTAS_CIMA"  	) //Rotas
	oView:SetOwnerView("VwGridROTD"  	, "ROTAS_MEIO"  	) //Total por Rota e Trato
	oView:SetOwnerView("VwKgTot3"  		, "PROGR_BAIXO_ESQ" ) //Total da Rota
	oView:SetOwnerView("VwGridZ06G"  	, "PROGR_CIMA"  	) //Lista de Currais
	oView:SetOwnerView("VwGridZ06D"  	, "LOTES_CENTER"	) //Relação de Tratos
	oView:SetOwnerView("VwKgTot2" 		, "LOTES_CIMA"  	) //Totais por Lote
	oView:SetOwnerView("VwGridZ05G" 	, "LOTES_BAIXO" 	) //Programação anterior
	oView:SetOwnerView("VwKgTot1"    	, "PROGR_BAIXO_DIR"	) //Totais por Rota

	oView:SetNoInsertLine("VwGridZ06G")
	oView:SetNoDeleteLine("VwGridZ06G")

	oView:SetNoInsertLine("VwGridZ06D")
	oView:SetNoDeleteLine("VwGridZ06D")

	oView:SetNoInsertLine("VwGridROTD")
	oView:SetNoDeleteLine("VwGridROTD")

	oView:SetNoInsertLine("VwGridROTG")

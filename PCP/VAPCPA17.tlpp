user function vap17trtValidZ06D()
	local aSaveLines 		:= FWSaveRows()  as Array
	local aArea     := GetArea() AS Array
	local oModel  	:= FWModelActive() as Object 
	local oGridZ06G := oModel:GetModel("MdGridZ06G") as Object 
	local oGridZ06D := oModel:GetModel("MdGridZ06D") as Object 
	local aKgMS     := {}
	local lRet      := .T.
	local nI
	local cDieta	:= ""
	local cDietaZ06G:= AllTrim(&(ReadVar()))
	local cSeq      := ""
	local nTotMN   	:= 0
	local nMegCal 	:= 0

	If !IsInCallStack("Custom.VAPCPA17.U_vap17trtValidZ06G")
		SB1->(DbSetOrder(1))
		if !(SB1->(DbSeek(FwxFilial("SB1")+PADR(cDietaZ06G, TamSx3("B1_COD")[1])))) .or. SB1->B1_X_TRATO!='1'
			Help(,, "Dieta Inválida",, "O código digitado não pertence a um produto válido ou esse produto não é uma dieta.", 1, 0,,,,,, {"Por favor digite uma dieta válida ou selecione." + CRLF + "<F3 Disponível>."})
			lRet := .F.
		endif

		if Z0R->Z0R_LOCK = '2' 
			Help(,, "OPERACAO NAO PERMITDA.",, "Não é possível alterar o trato pois ele já foi Publicado.", 1, 0,,,,,, {"Operação não permitida."})
			lRet := .F.
		elseif Z0R->Z0R_LOCK = '3' 
			Help(,, "OPERACAO NAO PERMITDA.",, "Não é possível alterar o trato pois ele foi Encerrado.", 1, 0,,,,,, {"Operação não permitida."})
			lRet := .F.
		endif

		IF lRet
			//if Z05->(DbSeek(FWxFilial("Z05")+DToS(Z0R->Z0R_DATA)+Z0R->Z0R_VERSAO+;
			//							PADR(oGridZ06G:GetValue("Z05_CURRAL"), TamSx3("Z05_CURRAL")[1])+;
			//							PADR(oGridZ06G:GetValue("Z06_LOTE"), TamSx3("Z06_LOTE")[1])))
			Z05->(DbGoTo(oGridZ06G:GetValue("Z05_RECNO")))

			if lRet := U_CanUseZ05()
			endif
		
		endif
	Endif
	
	if lRet
		aKgMS := U_DivTrato(Z05->Z05_KGMSDI, oGridZ06G:GetValue("Z05_NROTRA"))

		nQuantMN := u_CalcQtMN(cDietaZ06G, aKgMS[Val(oGridZ06D:GetValue("Z06_TRATO"))])
		nMCalTrt := U_GetMegaCal(cDietaZ06G) * aKgMS[Val(oGridZ06D:GetValue("Z06_TRATO"))]
		cSeq := U_GetSeq(cDietaZ06G)
		
		Z06->(DbGoTo(oGridZ06D:GetValue("Z06_RECNO")))
		
		RecLock("Z06", .F.)
			Z06->Z06_DIETA  := cDietaZ06G
			Z06->Z06_KGMSTR := aKgMS[Val(oGridZ06D:GetValue("Z06_TRATO"))]
			Z06->Z06_KGMNTR := nQuantMN
			Z06->Z06_MEGCAL := nMCalTrt
			Z06->Z06_KGMNT  := nQuantMN * Z05->Z05_CABECA
			Z06->Z06_SEQ    := cSeq
		Z06->(MsUnlock())

		oGridZ06D:LoadValue("Z06_KGMNTR", Z06->Z06_KGMNTR)
		oGridZ06D:LoadValue("Z06_MEGCAL", Z06->Z06_MEGCAL)
		oGridZ06D:LoadValue("Z06_KGMSTR", Z06->Z06_KGMSTR)
		oGridZ06D:LoadValue("Z06_KGMNT" , Z06->Z06_KGMNT )
	endif

	if !IsInCallStack("Custom.VAPCPA17.U_vap17trtValidZ06G")
		for nI := 1 to oGridZ06D:Length()
			oGridZ06D:GoLine(nI)

			if !AllTrim(oGridZ06D:GetValue("Z06_DIETA"))$cDieta
				cDieta += Iif(Empty(cDieta), "", ",") + AllTrim(oGridZ06D:GetValue("Z06_DIETA")) 
			endif

			nTotMN += oGridZ06D:GetValue("Z06_KGMNTR")
			nMegCal += oGridZ06D:GetValue("Z06_MEGCAL")
		next

		oGridZ06G:LoadValue("Z06_DIETA",cDieta)

		RecLock("Z05", .F.)
			Z05->Z05_MANUAL := "1"
			Z05->Z05_DIETA  := cDieta
			Z05->Z05_KGMNDI := nTotMN
			Z05->Z05_MEGCAL := nMegCal
			Z05->Z05_TOTMNI := Z05->Z05_KGMNDI
		MsUnlock()

	endif
	
	RestArea(aArea)
	FWRestRows(aSaveLines)
Return lRet


	local oStrTot2  := nil as object

	oModel 	:= FwLoadModel("custom.VAPCPA17.VAPCPA17")

	oStrZ05G   	:= Z05GrdVStr()
	oStrRotD 	:= ROTGrdVStr(1)
	oStrRotG 	:= ROTGrdVStr(2)
	oStrHide 	:= HidGrdVStr()

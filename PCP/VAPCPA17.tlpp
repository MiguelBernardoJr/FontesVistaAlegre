static function LoadHide(oModel, lCopia) as Array
    local aArea 	:= GetArea() as Array	
    Local aRet  	:= {} as Array
	local cZ0TROTA  := IIF(Type("cFiltroRota")!="U" .and. !Empty(cFiltroRota),cFiltroRota,(cTrbBrowse)->Z0T_ROTA)

	if Type("cFiltroEquip") == "U"
		cQry := " SELECT top 1 Z0T_ROTA, Z0S_EQUIP, ZV0_DESC" + CRLF
		cQry += " FROM "+RetSqlName("Z0T")+" Z0T" + CRLF
		cQry += " JOIN "+RetSqlName("Z0S")+" Z0S ON Z0S_FILIAL = Z0T_FILIAL" + CRLF
		cQry += " AND Z0S_DATA = Z0T_DATA" + CRLF
		cQry += " AND Z0S_ROTA = Z0T_ROTA" + CRLF
		cQry += " AND Z0S.D_E_L_E_T_ = ''" + CRLF
		cQry += " JOIN "+RetSqlName("ZV0")+" ZV0 ON ZV0_CODIGO = Z0S_EQUIP " + CRLF
		cQry += " AND ZV0.D_E_L_E_T_ = '' " + CRLF
		cQry += " WHERE Z0T_DATA = '"+dToS(Z0R->Z0R_DATA)+"'" + CRLF
		cQry += " AND Z0T.D_E_L_E_T_ = ''" + CRLF
		cQry += " AND Z0T_ROTA = '"+cZ0TROTA+"'" + CRLF
		cQry += " GROUP BY Z0T_ROTA,Z0S_EQUIP,ZV0_DESC" + CRLF
		cQry += " ORDER BY Z0T_ROTA" + CRLF

		cAlias := MpSysOpenQuery(cQry)
		
		aAdd(aRet, {AllTrim((cAlias)->Z0S_EQUIP),ALLTRIM((cAlias)->ZV0_DESC)})
		aAdd(aRet, {1})

		(cAlias)->(DbCloseArea())
	else
		aAdd(aRet, {AllTrim(cFiltroEquip),;
					ALLTRIM(Posicione("ZV0",1,FwxFilial("ZV0")+cFiltroEquip,"ZV0_DESC"));
					;//AllTrim(cZ0TROTA)
					})
		aAdd(aRet, {1})
	endif

    RestArea(aArea)
Return aRet

